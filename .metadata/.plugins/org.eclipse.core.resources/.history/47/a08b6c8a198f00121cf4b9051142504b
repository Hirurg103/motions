import java.nio.IntBuffer;

import javax.vecmath.Color4b;
import javax.vecmath.Color4f;

public class GraphicsObject {
	protected Color4b myColorID;
	private static long lastID = 0;
	
	public GraphicsObject() {
		this.myColorID = new Color4b();
		this.myColorID.setX((byte)(lastID % 256)); 
		this.myColorID.setY((byte)(lastID/256 % 256));
		this.myColorID.setZ((byte)(lastID/65536 % 256));
		this.myColorID.setW(18b);
		lastID++;
	}
	
	@Override
	public String toString() {
		return "(" + myColorID.getX() + ", " + myColorID.getY() + ", " +  myColorID.getZ() + ", " + myColorID.getW() + ")";
	}
}
