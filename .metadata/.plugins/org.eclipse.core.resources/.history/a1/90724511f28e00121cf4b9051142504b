
public class GraphicsObject {
	private int[] myColorID;
	private static long lastID = 0;
	
	public GraphicsObject() {
		this.myColorID = new int[3];
		this.myColorID[0] = lastID % 256;
		lastColorID[0]++;
		if(lastColorID[0] > 255) {
			lastColorID[0] = 0;
			lastColorID[1]++;
			if(lastColorID[1] > 255) {
				lastColorID[1] = 0;
				lastColorID[2]++;
			}
		}
	}
	
	@Override
	public String toString() {
		return myColorID[0] + " " + myColorID[1] + " " +  myColorID[2];
	}
}
