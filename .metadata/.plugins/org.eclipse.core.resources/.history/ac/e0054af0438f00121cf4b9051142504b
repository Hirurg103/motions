import java.util.ArrayList;


public class CompositeSkeletonPart extends SkeletonPart {
	private ArrayList<SkeletonPart> compositeParts;
	public CompositeSkeletonPart(String name) {
		super(name);
		this.compositeParts = new ArrayList<SkeletonPart>();
	}
	
	public void addCompositePart(SkeletonPart skeletonPart) {
		compositeParts.add(skeletonPart);
	}
	
}
