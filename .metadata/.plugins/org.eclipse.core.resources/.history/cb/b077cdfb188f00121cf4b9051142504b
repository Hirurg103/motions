import java.nio.IntBuffer;

import javax.vecmath.Color4b;
import javax.vecmath.Color4f;

public class GraphicsObject {
	protected Color4f myColorID;
	private static long lastID = 0;
	
	public GraphicsObject() {
		this.myColorID = new Color4f();
		this.myColorID.put((int)(lastID % 256));
		this.myColorID.put((int)(lastID/256 % 256));
		this.myColorID.put((int)(lastID/65536 % 256));
		this.myColorID.put(18);
		lastID++;
	}
	
	@Override
	public String toString() {
		return "(" + myColorID.get(0) + ", " + myColorID.get(1) + ", " +  myColorID.get(2) + ")";
	}
}
